{
  "name"        : "docpad-plugin-plugins",
  "description" : "DocPad plugin which streamlines the effort of developing plugins in a structured manner.",
  "homepage"    : "https://github.com/supersym/docpad-plugin-plugins",
  "version"     : "1.0.0",
  "keywords"    : [ "docpad-plugin", "plugins", "docpad", "workflow", "docpad development" ],
  "author"      : "Rob Jentzema <maildumpert@gmail.com> (hack.co.nl)",
  "maintainers" : [ "Rob Jentzema <maildumpert@gmail.com> (hack.co.nl)" ],
  "dependencies": {
    "coffee-script"     :     "latest",
    "commander"         :     "latest",
    "custom-logger"     :     "latest",
    "namespace.coffee"  :     "https://github.com/CodeCatalyst/namespace.coffee/tarball/master"
  },
  "bin": {
    "docpad-plugin": "./bin/docpad-plugins"
